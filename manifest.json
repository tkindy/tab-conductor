{
  "manifest_version": 3,
  "name": "Tab Conductor",
  "version": "1.1",

  "description": "Manage tabs efficiently.",

  "commands": {
    "move-to-new-window": {
      "suggested_key": {
        "default": "MacCtrl+W"
      }
    },
    "move-to-previous-window": {
      "suggested_key": {
        "default": "MacCtrl+Shift+W"
      }
    }
  },

  "background": {
    "scripts": ["background.js"],
    "type": "module"
  },

  "browser_specific_settings": {
    "gecko": {
      "id": "tabconductor@tylerkindy.com"
    }
  }
}
